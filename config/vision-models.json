      "options": {
        "temperature": 0.1,
        "maxTokens": 2048
      }
    },
    "llava-ollama": {
      "provider": "ollama",
      "baseUrl": "http://localhost:11434",
      "model": "llava",
      "description": "LLaVA - Fast vision-language model",
